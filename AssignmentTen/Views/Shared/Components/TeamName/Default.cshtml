@{
}
@model IEnumerable<Team>

<div style="height:75px;"></div>

<ul class="left-nav">
    <a href="/Home/Index">
        <li class="line-guy" id="link-Home">
            Home
        </li>
    </a>

    @foreach (Team x in Model)
    {
<a asp-controller="Home" asp-action="Index" asp-route-teamid="@x.TeamId"
   asp-route-teamname="@x.TeamName">
    <li id="link-@x.TeamName" class="line-guy">
        @x.TeamName
    </li>
</a>
    }

</ul>
